# AuditDNA Environment Configuration
# Copy this file to .env.local and configure your settings

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo
OPENAI_MAX_TOKENS=500
OPENAI_TEMPERATURE=0.7

# Optional: Custom OpenAI endpoint (for Azure OpenAI or other providers)
# OPENAI_ENDPOINT=https://your-custom-endpoint.openai.azure.com/

# Chat Configuration
CHAT_ENABLED=true
CHAT_MAX_MESSAGES=50
CHAT_RATE_LIMIT=60

# Development Settings
NODE_ENV=development
VITE_API_BASE_URL=http://localhost:3000